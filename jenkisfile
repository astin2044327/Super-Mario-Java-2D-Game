pipeline {
agent any 
stages {
stage ('git checkout') {
steps {
checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/astin2044327/Super-Mario-Java-2D-Game.git']]])

   }
   }
   
   stage ('jar file build') {
   steps {
   sh 'java -jar "SuperMiroGame.jar" '
   
   }
   }
   
   
   
   
   }
   }
   
